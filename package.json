{
  "name": "fcs-parser",
  "version": "0.0.2",
  "description": "Parse flow cytometry standard (FCS) files",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rollup -c && cheminfo-build --root FCSParser",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepublishOnly": "rollup -c",
    "test": "npm run test-coverage && npm run eslint",
    "test-coverage": "jest --coverage",
    "test-only": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/fcs.git"
  },
  "keywords": [],
  "author": "Javier Osorio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/fcs/issues"
  },
  "homepage": "https://github.com/cheminfo/fcs#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@types/jest": "^26.0.15",
    "cheminfo-build": "^1.1.8",
    "eslint": "^7.12.0",
    "eslint-config-cheminfo": "^5.2.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.1",
    "prettier": "^2.1.2",
    "rollup": "^2.32.1"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "iobuffer": "^5.0.2",
    "path": "^0.12.7"
  }
}
